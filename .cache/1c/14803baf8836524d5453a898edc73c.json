ginal":{"line":5,"column":0}},{"name":"TodoList","generated":{"line":68,"column":15},"source":"components/todolist.js","original":{"line":50,"column":15}}],"sources":{"components/todolist.js":"import React from \"react\";\nimport AddTodo from \"./addtodo\";\nimport DeleteTodo from \"./deletetodo\";\n\nconst TodoList = () => {\n    const [todos, setTodos] = React.useState([\n        {id: 1, text: \"wash dishes\", done: false},\n        {id: 2, text: \"do laundry\", done: false},\n        {id: 3, text: \"take a shower\", done: false},\n        {id: 4, text: \"sing a song\", done: false},\n    ]);\n    return (\n        <div className=\"todo\">\n            <h2>TodoList</h2>\n            <ListTodo todos={todos} setTodos={setTodos} />\n            <AddTodo setTodos={setTodos} />\n        </div>\n    );\n\n    function ListTodo({todos, setTodos}) {\n        function handleToggelTodo(todo) {\n            const updatedTodo = todos.map((t) =>\n                t.id === todo.id\n                    ? {\n                          ...t,\n                          done: !t.done,\n                      }\n                    : t,\n            );\n            setTodos(updatedTodo);\n        }\n\n        return (\n            <ul>\n                {todos.map((todo, setTodo) => (\n                    <li\n                        onDoubleClick={() => handleToggelTodo(todo)}\n                        style={{\n                            textDecoration: todo.done ? \"line-through\" : \"\",\n                        }}\n                        key={todo.id}>\n                        {todo.text}\n                        <DeleteTodo todo={todo} setTodos={setTodos} />\n                    </li>\n                ))}\n            </ul>\n        );\n    }\n};\nexport default TodoList;\n"},"lineCount":null}},"error":null,"hash":"b93556899a10da7e141634ead6900f09","cacheData":{"env":{}}}